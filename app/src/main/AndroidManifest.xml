<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="my.assignment.forthe.card.game.blackjack">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.BlackJack">
        <activity android:name="my.assignment.forthe.card.game.blackjack.Settings"></activity>
        <activity android:name="my.assignment.forthe.card.game.blackjack.GameOver" />
        <activity android:name="my.assignment.forthe.card.game.blackjack.GameActivity" />
        <activity android:name="my.assignment.forthe.card.game.blackjack.SplashScreen">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="my.assignment.forthe.card.game.blackjack.MainActivity" />
    </application>

</manifest>